<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" minHeight="400.0" minWidth="500.0"
	spacing="50.0" xmlns="http://javafx.com/javafx/20.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.VipDashboardController">
	<children>
		<Label fx:id="welcomeText" text="Welcome! Full Name">
			<font>
				<Font size="23.0" />
			</font>
		</Label>
		<HBox alignment="CENTER" spacing="20.0">
			<children>
				<Button fx:id="addPostButton" contentDisplay="CENTER"
					minHeight="50.0" minWidth="100.0" mnemonicParsing="false"
					onAction="#addPost" text="Add Post" textAlignment="CENTER" />
				<Button fx:id="retrievePostButton" minHeight="50.0"
					minWidth="100.0" mnemonicParsing="false" onAction="#retrievePost"
					text="Retrieve Post" textAlignment="CENTER" />
				<Button fx:id="removePostButton" minHeight="50.0"
					minWidth="100.0" mnemonicParsing="false" onAction="#removePost"
					text="Remove Post" textAlignment="CENTER" />
				<Button fx:id="editProfileButton" minHeight="50.0"
					minWidth="100.0" mnemonicParsing="false" onAction="#editProfile"
					text="Edit Profile" textAlignment="CENTER" />
			</children>
			<VBox.margin>
				<Insets />
			</VBox.margin>
		</HBox>
		<HBox alignment="CENTER" layoutX="10.0" layoutY="177.0"
			spacing="20.0">
			<children>
				<Button fx:id="topPostsButton" contentDisplay="CENTER"
					minHeight="50.0" minWidth="100.0" mnemonicParsing="false"
					onAction="#getTopPost" text="Top Posts" textAlignment="CENTER" />
				<Button fx:id="exportPostButton" minHeight="50.0"
					minWidth="100.0" mnemonicParsing="false" onAction="#exportPost"
					text="Export Post" textAlignment="CENTER" />
				<Button fx:id="dataVisualsButton" minHeight="50.0"
					minWidth="100.0" mnemonicParsing="false" onAction="#dataVisuals"
					text="Data Visuals" textAlignment="CENTER" />
				<Button fx:id="bulkImportButton" layoutX="330.0"
					layoutY="10.0" minHeight="50.0" minWidth="100.0"
					mnemonicParsing="false" onAction="#bulkImport" text="Bulk Import"
					textAlignment="CENTER" />
			</children>
		</HBox>
		<HBox alignment="CENTER" spacing="20.0">
			<children>
				<Button fx:id="logoutButton" contentDisplay="CENTER"
					minHeight="50.0" minWidth="100.0" mnemonicParsing="false"
					onAction="#logout" text="Log Out" textAlignment="CENTER" />
			</children>
		</HBox>
	</children>
</VBox>
